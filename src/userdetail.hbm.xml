<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.trade.UserDetail" table="userdetail">
     <id name="accountNumber" type="long" column="accountnumber" length="20" />
        <property name="userId" type="string" column="userid" not-null="true"  length="20" />
        <property name="fullName" type="string" column="fullname" not-null="true"  length="50" />
        <property name="pan" type="string" column="pan" not-null="true"  length="15" />
        <property name="aadharNumber" type="string" column="aadharnumber" not-null="true"  length="15" />
        </class>
</hibernate-mapping>